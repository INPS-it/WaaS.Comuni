			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>beneficiario stesso</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation>genitori affidatari/tutore</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
